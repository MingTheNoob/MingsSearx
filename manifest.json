{
    "manifest_version": 2,
    "name": "MingsSearx",
    "version": "1.0",
    "description": "MingsSearx",
    "developer": {
        "name": "MingsUtilities",
        "url": "https://mingsutilities.com/dir/web-tools/extensions/MingsSearx"
    },
    "browser_action": {
        "default_icon": "icons/icon.svg",
        "default_title": "MingsSearx"
    },
    "browser_specific_settings": {
        "gecko": {
            "id": "mingssearx@mingsutilities.com",
            "strict_min_version": "55.0"
        }
    },
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "MingsSearx",
            "search_url": "https://searx.mingsutilities.com/?q={searchTerms}",
            "keyword": "msx"
        }
    },
    "icons": {
        "16": "icons/16.png",
        "32": "icons/32.png",
        "48": "icons/48.png",
        "96": "icons/96.png",
        "128": "icons/128.png"
    }
}